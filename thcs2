//Tính tổng, hiệu, tích, thương của hai số nguyên

#include <stdio.h>

int main() {
    int a, b;
    printf("Nhap hai so nguyen: ");
    scanf("%d %d", &a, &b);

    printf("Tong: %d\n", a + b);
    printf("Hieu: %d\n", a - b);
    printf("Tich: %d\n", a * b);
    
    if (b != 0)
        printf("Thuong: %.2f\n", (float)a / b);
    else
        printf("Khong the chia cho 0.\n");

    return 0;
}


// Tính chu vi và diện tích hình chữ nhật

#include <stdio.h>

int main() {
    float dai, rong;
    printf("Nhap chieu dai va chieu rong: ");
    scanf("%f %f", &dai, &rong);

    float chuVi = 2 * (dai + rong);
    float dienTich = dai * rong;

    printf("Chu vi: %.2f\n", chuVi);
    printf("Dien tich: %.2f\n", dienTich);

    return 0;
}


//Tính diện tích tam giác khi biết 3 cạnh
#include <stdio.h>
#include <math.h>

int main() {
    float a, b, c;
    printf("Nhap 3 canh cua tam giac: ");
    scanf("%f %f %f", &a, &b, &c);

    if (a + b > c && a + c > b && b + c > a) {
        float p = (a + b + c) / 2;
        float area = sqrt(p * (p - a) * (p - b) * (p - c));
        printf("Dien tich tam giac: %.2f\n", area);
    } else {
        printf("Khong phai tam giac hop le.\n");
    }

    return 0;
}


// Tính giai thừa của một số nguyên

#include <stdio.h>

int main() {
    int n;
    long long giaithua = 1;
    printf("Nhap so nguyen n: ");
    scanf("%d", &n);

    if (n < 0) {
        printf("Khong tinh duoc giai thua cua so am.\n");
    } else {
        for (int i = 1; i <= n; i++) {
            giaithua *= i;
        }
        printf("%d! = %lld\n", n, giaithua);
    }

    return 0;
}


//Tính tổng các chữ số của một số nguyên

#include <stdio.h>

int main() {
    int n, sum = 0;
    printf("Nhap mot so nguyen: ");
    scanf("%d", &n);

    int temp = n > 0 ? n : -n;
    while (temp != 0) {
        sum += temp % 10;
        temp /= 10;
    }

    printf("Tong cac chu so cua %d la: %d\n", n, sum);
    return 0;
}

//doi nhiet do

#include <stdio.h>

int main() {
    int choice;
    float temp;

    printf("1. Doi C sang F\n");
    printf("2. Doi F sang C\n");
    printf("Chon: ");
    scanf("%d", &choice);

    if (choice == 1) {
        printf("Nhap nhiet do (do C): ");
        scanf("%f", &temp);
        printf("Do F = %.2f\n", temp * 9 / 5 + 32);
    } else if (choice == 2) {
        printf("Nhap nhiet do (do F): ");
        scanf("%f", &temp);
        printf("Do C = %.2f\n", (temp - 32) * 5 / 9);
    } else {
        printf("Lua chon khong hop le.\n");
    }

    return 0;
}


//in ra so dao nguoc

#include <stdio.h>

int main() {
    int n, reversed = 0;
    printf("Nhap so nguyen N: ");
    scanf("%d", &n);

    int temp = n > 0 ? n : -n;
    while (temp != 0) {
        reversed = reversed * 10 + temp % 10;
        temp /= 10;
    }

    if (n < 0) reversed = -reversed;
    printf("So dao nguoc: %d\n", reversed);

    return 0;
}


//kiem tra so nguyen to

#include <stdio.h>
#include <stdbool.h>

bool isPrime(int n) {
    if (n < 2) return false;
    for (int i = 2; i*i <= n; i++) {
        if (n % i == 0) return false;
    }
    return true;
}

int main() {
    int n;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);

    if (isPrime(n))
        printf("%d la so nguyen to.\n", n);
    else
        printf("%d khong phai la so nguyen to.\n", n);

    return 0;
}


//Tính tổng các số lẻ từ 1 đến n

#include <stdio.h>

int main() {
    int n, sum = 0;
    printf("Nhap so n: ");
    scanf("%d", &n);

    for (int i = 1; i <= n; i += 2) {
        sum += i;
    }

    printf("Tong cac so le tu 1 den %d la %d\n", n, sum);
    return 0;
}


//Tính tổng các số chẵn từ 1 đến n

#include <stdio.h>

int main() {
    int n, sum = 0;
    printf("Nhap so n: ");
    scanf("%d", &n);

    for (int i = 2; i <= n; i += 2) {
        sum += i;
    }

    printf("Tong cac so chan tu 1 den %d la %d\n", n, sum);
    return 0;
}



//Tính tổng của dãy số S = 1 + 1/2 + 1/3 + ... + 1/n

#include <stdio.h>

int main() {
    int n;
    double sum = 0.0;

    printf("Nhap n: ");
    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {
        sum += 1.0 / i;
    }

    printf("Tong S = %.5lf\n", sum);
    return 0;
}


//Tính lũy thừa a^b (không dùng hàm thư viện)

#include <stdio.h>

int main() {
    int a, b;
    long long result = 1;

    printf("Nhap a va b: ");
    scanf("%d %d", &a, &b);

    for (int i = 1; i <= b; i++) {
        result *= a;
    }

    printf("%d^%d = %lld\n", a, b, result);
    return 0;
}


//Kiểm tra số hoàn hảo (Perfect number)

#include <stdio.h>

int main() {
    int n, sum = 0;
    printf("Nhap so nguyen n: ");
    scanf("%d", &n);

    for (int i = 1; i < n; i++) {
        if (n % i == 0) {
            sum += i;
        }
    }

    if (sum == n)
        printf("%d la so hoan hao.\n", n);
    else
        printf("%d khong phai la so hoan hao.\n", n);

    return 0;
}


//Tính tổng các chữ số chẵn của một số

#include <stdio.h>

int main() {
    int n, sum = 0;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);

    int temp = n > 0 ? n : -n;
    while (temp != 0) {
        int digit = temp % 10;
        if (digit % 2 == 0)
            sum += digit;
        temp /= 10;
    }

    printf("Tong chu so chan: %d\n", sum);
    return 0;
}


//Tính tổng các chữ số lẻ của một số
#include <stdio.h>

int main() {
    int n, sum = 0;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);

    int temp = n > 0 ? n : -n;
    while (temp != 0) {
        int digit = temp % 10;
        if (digit % 2 != 0)
            sum += digit;
        temp /= 10;
    }

    printf("Tong chu so le: %d\n", sum);
    return 0;
}


//Tìm Ước Chung Lớn Nhất (GCD) và Bội Chung Nhỏ Nhất (LCM)

#include <stdio.h>

int ucln(int a, int b) {
    while (b != 0) {
        int r = a % b;
        a = b;
        b = r;
    }
    return a;
}

int bcnn(int a, int b) {
    return (a * b) / ucln(a, b);
}

int main() {
    int a, b;
    printf("Nhap hai so nguyen: ");
    scanf("%d %d", &a, &b);

    printf("UCLN: %d\n", ucln(a, b));
    printf("BCNN: %d\n", bcnn(a, b));

    return 0;
}

//so fibonanchi thu n

#include <stdio.h>

int fibonacci(int n) {
    if (n <= 1) return n;
    int a = 0, b = 1, c;
    for (int i = 2; i <= n; i++) {
        c = a + b;
        a = b;
        b = c;
    }
    return b;
}

int main() {
    int n;
    printf("Nhap n: ");
    scanf("%d", &n);

    printf("So Fibonacci thu %d la: %d\n", n, fibonacci(n));
    return 0;
}



//Tính căn bậc hai gần đúng của một số (không dùng sqrt)
#include <stdio.h>

double canBacHai(double x) {
    double guess = x / 2, epsilon = 0.00001;

    while ((guess * guess - x) > epsilon || (x - guess * guess) > epsilon) {
        guess = (guess + x / guess) / 2;
    }

    return guess;
}

int main() {
    double x;
    printf("Nhap so x: ");
    scanf("%lf", &x);

    if (x < 0) {
        printf("Khong tinh duoc can bac hai cua so am.\n");
    } else {
        printf("Can bac hai gan dung: %.5lf\n", canBacHai(x));
    }

    return 0;
}


//chuyen sang he nhi phan

#include <stdio.h>

void doiNhiPhan(int n) {
    if (n == 0) {
        printf("0");
        return;
    }

    int binary[32], i = 0;

    while (n > 0) {
        binary[i++] = n % 2;
        n /= 2;
    }

    for (int j = i - 1; j >= 0; j--)
        printf("%d", binary[j]);
}

int main() {
    int n;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);

    printf("Nhi phan: ");
    doiNhiPhan(n);
    printf("\n");

    return 0;
}





//Chuyển số nguyên sang hệ bát phân (cơ số 8)


#include <stdio.h>

void doiBatPhan(int n) {
    if (n == 0) {
        printf("0");
        return;
    }

    int octal[32], i = 0;

    while (n > 0) {
        octal[i++] = n % 8;
        n /= 8;
    }

    for (int j = i - 1; j >= 0; j--)
        printf("%d", octal[j]);
}

int main() {
    int n;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);

    printf("Bat phan: ");
    doiBatPhan(n);
    printf("\n");

    return 0;
}



