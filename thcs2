    ("Bài 21: Chuyển số nguyên sang hệ thập lục phân", """
// Bài 21: Chuyển số nguyên sang hệ thập lục phân
#include <stdio.h>

void doiThapLucPhan(int n) {
    char hex[32];
    int i = 0;

    if (n == 0) {
        printf("0");
        return;
    }

    while (n != 0) {
        int temp = n % 16;
        if (temp < 10)
            hex[i++] = temp + '0';
        else
            hex[i++] = temp - 10 + 'A';
        n /= 16;
    }

    for (int j = i - 1; j >= 0; j--)
        printf("%c", hex[j]);
}

int main() {
    int n;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);

    printf("He thap luc phan: ");
    doiThapLucPhan(n);
    printf("\\n");

    return 0;
}
"""),
    ("Bài 22: Chuyển số nguyên sang hệ M", """
// Bài 22: Chuyển số nguyên sang hệ M
#include <stdio.h>

void doiHeCoSoM(int n, int M) {
    int digits[64], i = 0;

    if (M < 2 || M > 36) {
        printf("Co so M khong hop le.\\n");
        return;
    }

    if (n == 0) {
        printf("0");
        return;
    }

    while (n > 0) {
        int r = n % M;
        digits[i++] = r;
        n /= M;
    }

    for (int j = i - 1; j >= 0; j--) {
        if (digits[j] < 10)
            printf("%d", digits[j]);
        else
            printf("%c", digits[j] - 10 + 'A');
    }
}

int main() {
    int n, M;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);
    printf("Nhap co so M (2-36): ");
    scanf("%d", &M);

    printf("He co so %d: ", M);
    doiHeCoSoM(n, M);
    printf("\\n");

    return 0;
}
"""),
    ("Bài 23: Tìm số nguyên tố thứ n", """
// Bài 23: Tìm số nguyên tố thứ n
#include <stdio.h>
#include <stdbool.h>

bool isPrime(int n) {
    if (n < 2) return false;
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0)
            return false;
    }
    return true;
}

int main() {
    int n, count = 0, number = 2;
    printf("Nhap n: ");
    scanf("%d", &n);

    while (1) {
        if (isPrime(number)) {
            count++;
            if (count == n) break;
        }
        number++;
    }

    printf("So nguyen to thu %d la: %d\\n", n, number);
    return 0;
}
"""),
