// Bài 67: Chuyển đổi đơn vị: km → m, m → cm, kg → g...
#include <stdio.h>

int main() {
    int chon;
    float val;
    printf("1. km -> m\n2. m -> cm\n3. kg -> g\nChon chuc nang: ");
    scanf("%d", &chon);
    printf("Nhap gia tri: ");
    scanf("%f", &val);
    switch (chon) {
        case 1: printf("%.2f km = %.2f m\n", val, val * 1000); break;
        case 2: printf("%.2f m = %.2f cm\n", val, val * 100); break;
        case 3: printf("%.2f kg = %.2f g\n", val, val * 1000); break;
        default: printf("Lua chon khong hop le\n");
    }
    return 0;
}


// Bài 68: Nhập điểm (0–10), dùng switch in ra nhận xét
#include <stdio.h>

int main() {
    int diem;
    printf("Nhap diem (0-10): ");
    scanf("%d", &diem);
    switch (diem) {
        case 10: printf("Xuat sac\n"); break;
        case 9: case 8: printf("Gioi\n"); break;
        case 7: case 6: printf("Kha\n"); break;
        case 5: printf("Trung binh\n"); break;
        default:
            if (diem >= 0 && diem < 5)
                printf("Yeu\n");
            else
                printf("Diem khong hop le\n");
    }
    return 0;
}



