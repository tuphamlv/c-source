int main() {
    for (int i = 2; i <= 9; i++) {
        printf("\nBang cuu chuong %d:\n", i);
        for (int j = 1; j <= 10; j++)
            printf("%d x %d = %d\n", i, j, i*j);
    }
    return 0;
}

// Bài 80: Đếm có bao nhiêu số chia hết cho 7 trong khoảng 1 đến n
#include <stdio.h>

int main() {
    int n, dem = 0;
    printf("Nhap n: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++)
        if (i % 7 == 0)
            dem++;
    printf("Co %d so chia het cho 7\n", dem);
    return 0;
}


// Bài 81: Kiểm tra một số có phải là số nguyên tố
#include <stdio.h>
#include <math.h>

int main() {
    int n, i, isPrime = 1;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);
    if (n < 2) isPrime = 0;
    for (i = 2; i <= sqrt(n); i++)
        if (n % i == 0) {
            isPrime = 0;
            break;
        }
    if (isPrime)
        printf("%d la so nguyen to\n", n);
    else
        printf("%d khong la so nguyen to\n", n);
    return 0;
}

// Bài 82: In ra các số nguyên tố từ 1 đến n
#include <stdio.h>
#include <math.h>

int isPrime(int n) {
    if (n < 2) return 0;
    for (int i = 2; i <= sqrt(n); i++)
        if (n % i == 0)
            return 0;
    return 1;
}

int main() {
    int n;
    printf("Nhap n: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++)
        if (isPrime(i))
            printf("%d ", i);
    return 0;
}

// Bài 83: Tính tổng các chữ số của một số nguyên
#include <stdio.h>

int main() {
    int n, tong = 0;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);
    while (n != 0) {
        tong += n % 10;
        n /= 10;
    }
    printf("Tong chu so = %d\n", tong);
    return 0;
}

// Bài 84: Tính tổng các chữ số là số nguyên tố của một số nguyên
#include <stdio.h>

int isPrimeDigit(int n) {
    return (n == 2 || n == 3 || n == 5 || n == 7);
}

int main() {
    int n, tong = 0;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);
    while (n != 0) {
        int d = n % 10;
        if (isPrimeDigit(d)) tong += d;
        n /= 10;
    }
    printf("Tong chu so nguyen to = %d\n", tong);
    return 0;
}

// Bài 85: Đảo ngược một số nguyên (ví dụ 123 -> 321)
#include <stdio.h>

int main() {
    int n, dao = 0;
    printf("Nhap so: ");
    scanf("%d", &n);
    while (n != 0) {
        dao = dao * 10 + n % 10;
        n /= 10;
    }
    printf("So dao nguoc: %d\n", dao);
    return 0;
}

// Bài 86: Tính tổng của dãy: S = 1 + 1/2 + 1/3 + ... + 1/n
#include <stdio.h>

int main() {
    int n;
    double S = 0;
    printf("Nhap n: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++)
        S += 1.0 / i;
    printf("Tong S = %.5lf\n", S);
    return 0;
}

// Bài 87: In ra tam giác sao - tam giác vuông góc vuông trên trái
#include <stdio.h>

int main() {
    int n;
    printf("Nhap chieu cao tam giac: ");
    scanf("%d", &n);
    for (int i = n; i >= 1; i--) {
        for (int j = 1; j <= i; j++)
            printf("* ");
        printf("\n");
    }
    return 0;
}

// Bài 88: In ra dãy Fibonacci với n phần tử đầu tiên
#include <stdio.h>

int main() {
    int n, a = 0, b = 1;
    printf("Nhap so phan tu: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        printf("%d ", a);
        int next = a + b;
        a = b;
        b = next;
    }
    return 0;
}

// Bài 89: Kiểm tra số hoàn hảo
#include <stdio.h>

int main() {
    int n, sum = 0;
    printf("Nhap so nguyen: ");
    scanf("%d", &n);
    for (int i = 1; i < n; i++)
        if (n % i == 0)
            sum += i;
    if (sum == n)
        printf("%d la so hoan hao\n", n);
    else
        printf("%d khong la so hoan hao\n", n);
    return 0;
}

// Bài 90: In ra bảng số nhị phân từ 1 đến n
#include <stdio.h>

void printBinary(int n) {
    if (n == 0) return;
    printBinary(n / 2);
    printf("%d", n % 2);
}

int main() {
    int n;
    printf("Nhap n: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        printBinary(i);
        printf("\n");
    }
    return 0;
}



// Bài 91: Kiểm tra số chính phương
#include <stdio.h>
#include <math.h>

int main() {
    int n;
    printf("Nhap so: ");
    scanf("%d", &n);
    int can = sqrt(n);
    if (can * can == n)
        printf("%d la so chinh phuong\n", n);
    else
        printf("%d khong la so chinh phuong\n", n);
    return 0;
}

// Bài 92: Tìm ước chung lớn nhất của hai số
#include <stdio.h>

int UCLN(int a, int b) {
    while (b != 0) {
        int r = a % b;
        a = b;
        b = r;
    }
    return a;
}

int main() {
    int a, b;
    printf("Nhap hai so: ");
    scanf("%d%d", &a, &b);
    printf("UCLN = %d\n", UCLN(a, b));
    return 0;
}

// Bài 93: Tìm bội chung nhỏ nhất của hai số
#include <stdio.h>

int UCLN(int a, int b) {
    while (b != 0) {
        int r = a % b;
        a = b;
        b = r;
    }
    return a;
}

int main() {
    int a, b;
    printf("Nhap hai so: ");
    scanf("%d%d", &a, &b);
    int ucln = UCLN(a, b);
    int bcnn = (a * b) / ucln;
    printf("BCNN = %d\n", bcnn);
    return 0;
}

// Bài 94: In tam giác vuông góc dưới trái
#include <stdio.h>

int main() {
    int n;
    printf("Nhap chieu cao: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++)
            printf("* ");
        printf("\n");
    }
    return 0;
}

// Bài 95: In tam giác vuông góc trên phải
#include <stdio.h>

int main() {
    int n;
    printf("Nhap chieu cao: ");
    scanf("%d", &n);
    for (int i = n; i >= 1; i--) {
        for (int j = 1; j < n - i + 1; j++)
            printf("  ");
        for (int j = 1; j <= i; j++)
            printf("* ");
        printf("\n");
    }
    return 0;
}

// Bài 96: In tam giác cân
#include <stdio.h>

int main() {
    int n;
    printf("Nhap chieu cao: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n - i; j++)
            printf(" ");
        for (int j = 1; j <= 2 * i - 1; j++)
            printf("*");
        printf("\n");
    }
    return 0;
}

// Bài 97: Kiểm tra chuỗi Palindrome (đảo ngược vẫn giống nhau)
#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    printf("Nhap chuoi: ");
    gets(s);
    int l = 0, r = strlen(s) - 1;
    int isPalin = 1;
    while (l < r) {
        if (s[l] != s[r]) {
            isPalin = 0;
            break;
        }
        l++;
        r--;
    }
    if (isPalin)
        printf("Chuoi Palindrome\n");
    else
        printf("Khong phai Palindrome\n");
    return 0;
}

// Bài 98: Đếm số lần xuất hiện của ký tự trong chuỗi
#include <stdio.h>
#include <string.h>

int main() {
    char s[100], c;
    printf("Nhap chuoi: ");
    gets(s);
    printf("Nhap ky tu can dem: ");
    scanf("%c", &c);
    int dem = 0;
    for (int i = 0; s[i]; i++)
        if (s[i] == c)
            dem++;
    printf("Ky tu '%c' xuat hien %d lan\n", c, dem);
    return 0;
}

// Bài 99: In các ký tự không trùng nhau trong chuỗi
#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    printf("Nhap chuoi: ");
    gets(s);
    int freq[256] = {0};
    for (int i = 0; s[i]; i++)
        freq[(unsigned char)s[i]]++;
    printf("Cac ky tu khong trung lap: ");
    for (int i = 0; s[i]; i++)
        if (freq[(unsigned char)s[i]] == 1)
            printf("%c ", s[i]);
    return 0;
}

// Bài 100: Đảo ngược chuỗi
#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    printf("Nhap chuoi: ");
    gets(s);
    int len = strlen(s);
    for (int i = len - 1; i >= 0; i--)
        printf("%c", s[i]);
    printf("\n");
    return 0;
}






