// Bài 70: Tính tiền lương theo cấp bậc chọn từ menu (A, B, C...)
#include <stdio.h>

int main() {
    char cap;
    printf("Nhap cap bac (A/B/C): ");
    scanf(" %c", &cap);
    switch (cap) {
        case 'A': case 'a': printf("Luong: 10,000,000 VND\n"); break;
        case 'B': case 'b': printf("Luong: 8,000,000 VND\n"); break;
        case 'C': case 'c': printf("Luong: 6,000,000 VND\n"); break;
        default: printf("Cap bac khong hop le\n");
    }
    return 0;
}




