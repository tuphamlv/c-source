// Bài 39: Tính lũy thừa a^b (1: vòng lặp, 2: pow)
#include <stdio.h>
#include <math.h>

int main() {
    int chon;
    int a, b;
    printf("Nhap a, b: ");
    scanf("%d %d", &a, &b);
    printf("Chon cach tinh (1: vong lap, 2: pow): ");
    scanf("%d", &chon);

    if (chon == 1) {
        int kq = 1;
        for (int i = 0; i < b; i++)
            kq *= a;
        printf("Ket qua: %d\\n", kq);
    } else if (chon == 2) {
        printf("Ket qua: %.2f\\n", pow(a, b));
    } else {
        printf("Lua chon khong hop le\\n");
    }

    return 0;
}
"""),
    ("Bài 40: Tính tổng các số từ 1 đến n (dùng công thức)", """
// Bài 40: Tính tổng các số từ 1 đến n (dùng công thức)
#include <stdio.h>

int main() {
    int n;
    printf("Nhap n: ");
    scanf("%d", &n);
    int tong = n * (n + 1) / 2;
    printf("Tong = %d\\n", tong);
    return 0;
}
""")
]


// Bài 41: Tính hiệu của tổng các số chẵn và tổng các số lẻ từ 1 đến n
#include <stdio.h>

int main() {
    int n, tongChan = 0, tongLe = 0;
    printf("Nhap n: ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        if (i % 2 == 0)
            tongChan += i;
        else
            tongLe += i;
    }
    printf("Hieu = %d\n", tongChan - tongLe);
    return 0;
}


// Bài 42: Phép chia có kiểm tra chia cho 0
#include <stdio.h>

int main() {
    float a, b;
    printf("Nhap hai so: ");
    scanf("%f %f", &a, &b);
    if (b == 0)
        printf("Khong the chia cho 0\n");
    else
        printf("Thuong = %.2f\n", a / b);
    return 0;
}


// Bài 43: Nhập ba số, tìm tổng của hai số lớn nhất
#include <stdio.h>

int main() {
    int a, b, c, tong;
    printf("Nhap 3 so: ");
    scanf("%d %d %d", &a, &b, &c);
    if (a <= b && a <= c)
        tong = b + c;
    else if (b <= a && b <= c)
        tong = a + c;
    else
        tong = a + b;
    printf("Tong hai so lon nhat = %d\n", tong);
    return 0;
}


// Bài 44: Tính giá trị biểu thức (a + b)^2 hoặc (a - b)^2
#include <stdio.h>

int main() {
    int a, b, chon;
    printf("Nhap a, b: ");
    scanf("%d %d", &a, &b);
    printf("Chon 1: (a + b)^2 | Chon 2: (a - b)^2: ");
    scanf("%d", &chon);
    if (chon == 1)
        printf("Ket qua = %d\n", (a + b) * (a + b));
    else if (chon == 2)
        printf("Ket qua = %d\n", (a - b) * (a - b));
    else
        printf("Lua chon khong hop le\n");
    return 0;
}

// Bài 45: Tính giá trị biểu thức a^2 + b^2 + 2ab
#include <stdio.h>

int main() {
    int a, b;
    printf("Nhap a, b: ");
    scanf("%d %d", &a, &b);
    int kq = a * a + b * b + 2 * a * b;
    printf("Ket qua = %d\n", kq);
    return 0;
}


// Bài 46: Hoán đổi hai số không dùng biến tạm
#include <stdio.h>

int main() {
    int a, b;
    printf("Nhap a, b: ");
    scanf("%d %d", &a, &b);
    a = a + b;
    b = a - b;
    a = a - b;
    printf("Sau khi hoan doi: a = %d, b = %d\n", a, b);
    return 0;
}


// Bài 47: Tính giá trị tuyệt đối
#include <stdio.h>

int main() {
    int n;
    printf("Nhap mot so nguyen: ");
    scanf("%d", &n);
    printf("Gia tri tuyet doi = %d\n", n < 0 ? -n : n);
    return 0;
}


// Bài 48: Nhập giờ, phút, giây → tính tổng số giây
#include <stdio.h>

int main() {
    int h, m, s;
    printf("Nhap gio, phut, giay: ");
    scanf("%d %d %d", &h, &m, &s);
    int tong = h * 3600 + m * 60 + s;
    printf("Tong giay = %d\n", tong);
    return 0;
}


// Bài 49: Nhập tổng số giây → tính ra giờ, phút, giây
#include <stdio.h>

int main() {
    int total;
    printf("Nhap tong so giay: ");
    scanf("%d", &total);
    int h = total / 3600;
    int m = (total % 3600) / 60;
    int s = total % 60;
    printf("%02d:%02d:%02d\n", h, m, s);
    return 0;
}


