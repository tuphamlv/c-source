}


// Bài 63: Nhập số từ 1 đến 7, in ra thứ trong tuần
#include <stdio.h>

int main() {
    int thu;
    printf("Nhap so tu 1 den 7: ");
    scanf("%d", &thu);
    switch (thu) {
        case 1: printf("Chu nhat\n"); break;
        case 2: printf("Thu hai\n"); break;
        case 3: printf("Thu ba\n"); break;
        case 4: printf("Thu tu\n"); break;
        case 5: printf("Thu nam\n"); break;
        case 6: printf("Thu sau\n"); break;
        case 7: printf("Thu bay\n"); break;
        default: printf("Khong hop le\n");
    }
    return 0;
}



// Bài 64: Nhập tháng, in ra số ngày trong tháng (xét năm nhuận nếu là tháng 2)
#include <stdio.h>

int main() {
    int thang, nam;
    printf("Nhap thang va nam: ");
    scanf("%d %d", &thang, &nam);
    int ngay;
    switch (thang) {
        case 1: case 3: case 5: case 7:
        case 8: case 10: case 12:
            ngay = 31; break;
        case 4: case 6: case 9: case 11:
            ngay = 30; break;
        case 2:
            if ((nam % 4 == 0 && nam % 100 != 0) || (nam % 400 == 0))
                ngay = 29;
            else
                ngay = 28;
            break;
        default:
            printf("Thang khong hop le\n");
            return 0;
    }
    printf("Thang %d nam %d co %d ngay\n", thang, nam, ngay);
    return 0;
}




// Bài 65: Menu lựa chọn phép tính cộng, trừ, nhân, chia 2 số
#include <stdio.h>

int main() {
    float a, b;
    int chon;
    printf("Nhap hai so: ");
    scanf("%f %f", &a, &b);
    printf("1. Cong\n2. Tru\n3. Nhan\n4. Chia\nChon phep tinh: ");
    scanf("%d", &chon);
    switch (chon) {
        case 1: printf("Tong = %.2f\n", a + b); break;
        case 2: printf("Hieu = %.2f\n", a - b); break;
        case 3: printf("Tich = %.2f\n", a * b); break;
        case 4:
            if (b != 0)
                printf("Thuong = %.2f\n", a / b);
            else
                printf("Loi: chia cho 0\n");
            break;
        default: printf("Lua chon khong hop le\n");
    }
    return 0;
}



