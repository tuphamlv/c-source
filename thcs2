    float a, b;
    printf("Nhap a, b: ");
    scanf("%f %f", &a, &b);

    if (a == 0) {
        if (b == 0)
            printf("Phuong trinh vo so nghiem\\n");
        else
            printf("Phuong trinh vo nghiem\\n");
    } else {
        printf("Nghiem x = %.2f\\n", -b / a);
    }

    return 0;
}
