        printf("Khong chia het cho ca 3 va 5\n");
    return 0;
}



// Bài 61: Nhập 3 số nguyên, kiểm tra có tạo thành tam giác vuông không
#include <stdio.h>

int main() {
    int a, b, c;
    printf("Nhap 3 so nguyen: ");
    scanf("%d %d %d", &a, &b, &c);
    int a2 = a*a, b2 = b*b, c2 = c*c;
    if ((a + b > c) && (a + c > b) && (b + c > a)) {
        if (a2 + b2 == c2 || a2 + c2 == b2 || b2 + c2 == a2)
            printf("La tam giac vuong\n");
        else
            printf("Khong phai tam giac vuong\n");
    } else {
        printf("Khong phai tam giac\n");
    }
    return 0;
}


