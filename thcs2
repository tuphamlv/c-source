    scanf("%d %d %d", &a, &b, &c);
    if (a + b > c && a + c > b && b + c > a)
        printf("Day la tam giac\n");
    else
        printf("Khong phai tam giac\n");
    return 0;
}


// Bài 54: Kiểm tra ký tự là nguyên âm hay phụ âm
#include <stdio.h>

int main() {
    char ch;
    printf("Nhap mot ky tu: ");
    scanf(" %c", &ch);
    if (ch >= 'A' && ch <= 'Z') ch += 32; // chuyển sang chữ thường
    if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u')
        printf("Ky tu la nguyen am\n");
    else
        printf("Ky tu la phu am\n");
    return 0;
}


